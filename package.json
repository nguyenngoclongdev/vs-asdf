        "ffmpeg",
        "firebase",
        "gcloud",
        "gradle",
        "ibmcloud",
        "java",
        "kafka",
        "kotlin",
        "kube",
        "lua",
        "make",
        "mongo",
        "mysql",
        "node",
        "pnpm",
        "postgres",
        "r",
        "powershell",
